/*show css*/
.wrap {
    position: relative;
    margin-top: 25px;

    text-align:left;
}

.wrap > article {
    margin-bottom: -1px;
    box-sizing: border-box;
    border: 1px solid #e3e3e3;
    background-color: #fff;
}
.wrap > article > a.article {
    display: block;
    padding: 15px;
    color: #666;
    text-decoration: none;
}
.wrap > article h2.medium {
    margin-bottom: 5px;
    line-height: 18px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 18px;
    font-weight: normal;
    color: #292929;
    word-break: break-all;
}
.wrap > article p.small {
    margin-bottom: 5px;
    max-height: 30px;
    line-height: 15px;
    white-space: normal;
    overflow: hidden;
    color: #a6a6a6;
    font-size: 12px;
}
.wrap > article time.small {
    float: left;
    margin-right: 5px;
    height: 15px;
    line-height: 15px;
    font-size: 11px;
    color: #a6a6a6;
    letter-spacing: 0;
}
.wrap > article h3.small {
    float: left;
    max-width: 90px;
    height: 15px;
    line-height: 15px;
    font-size: 11px;
    font-weight: normal;
    letter-spacing: 0;
    color: #292929;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.wrap > article ul.status {
    float: right;
}
.wrap > article ul.status li {
    float: left;
    margin-left: 10px;
    padding-left: 15px;
    height: 15px;
    line-height: 15px;
    color: #a6a6a6;
    font-size: 12px;
    background-repeat: no-repeat;
    background-position: left center;
    background-size: 12px 12px;
    cursor: pointer;
}
.wrap > article ul.status li.vote {
    color: #0505aa;
}
.wrap > article ul.status li.comment {
    color: #03b1ef;
   
}
.wrap > article hr {
    clear: both;
    height: 0;
    border: 0;
    width:100%;
}
